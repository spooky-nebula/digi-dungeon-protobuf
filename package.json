{
  "name": "digi-dungeon-protobuf",
  "version": "0.1.6",
  "description": "Digi Dungeon API protocol buffers",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublish": "tsc --build --clean | tsc --project tsconfig.build.json | copyfiles -u 1 src/*.proto src/**/*.proto dist | npm pack",
    "build": "tsc --build --clean | tsc | copyfiles -u 1 src/*.proto src/**/*.proto dist-dev",
    "test": "npm build | node ./dist-dev/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spooky-nebula/digi-dungeon-protobuf.git"
  },
  "keywords": [
    "protobuff"
  ],
  "author": "Pedro Pires",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spooky-nebula/digi-dungeon-protobuf/issues"
  },
  "homepage": "https://github.com/spooky-nebula/digi-dungeon-protobuf#readme",
  "devDependencies": {
    "@types/node": "^16.10.1",
    "copyfiles": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "protobufjs": "^6.11.2"
  }
}
